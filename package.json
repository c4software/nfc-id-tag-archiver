{
  "name": "nfc-id-tag-archiver",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "cordova_build": "npm run build && cd cordova_app && cordova build && cd ../",
    "cordova_run": "npm run build && cd cordova_app && cordova run && cd ../",
    "cordova_build_release": "npm run build && cd cordova_app && cordova build android --release -- --keystore=../file.keystore --alias=nfc-id-tag-archiver --storePassword='INSERT_PASS_HERE' --password='INSERT_PASS_HERE' && cd ../",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "instascan": "git+https://github.com/schmich/instascan.git#master",
    "vue": "^2.6.6",
    "vue-i18n": "^8.8.2",
    "vue-router": "^3.0.2",
    "vuetify": "^1.5.1",
    "vuetify-vuejs-confirmdialog": "0.0.15"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-i18n": "^0.5.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.6",
    "vuetify-loader": "^1.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
